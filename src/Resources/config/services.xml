<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ASDispositionControl\Core\Api\Utilities\MailServiceHelper" public="true">      
            <argument id="Shopware\Core\Content\MailTemplate\Service\MailService" type="service"/>
        </service>
        <service id="SynlabOrderInterface\Core\Api\OrderInterfaceController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="ASDispositionControl\Core\Api\Utilities\MailServiceHelper" />
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
    </services>
</container>